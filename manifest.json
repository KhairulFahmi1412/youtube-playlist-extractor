{
  "manifest_version": 3,
  "name": "YouTube Playlist Extractor",
  "version": "1.0",
  "description": "Extracts song names from playlist video based on timestamps.",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://*.youtube.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon.png"
    }
  }
}
